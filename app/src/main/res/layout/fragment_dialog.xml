    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/myScrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/rl_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:background="@drawable/rounded_corners"
        android:visibility="invisible"
        tools:ignore="MissingPrefix"
        tools:visibility="visible">

        <View
            android:id="@+id/view11"
            android:layout_width="20dp"
            android:layout_height="3dp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="18dp"
            android:background="@android:color/darker_gray" />

        <ImageView
            android:id="@+id/imggg"
            android:layout_width="30dp"
            android:layout_height="38dp"
            android:layout_below="@id/view11"
            android:layout_margin="25dp"
            android:src="@drawable/note2" />

        <EditText
            android:id="@+id/new_edittext"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/view11"
            android:layout_marginTop="25dp"
            android:layout_marginRight="60dp"
            android:layout_marginBottom="15dp"
            android:layout_toEndOf="@+id/imggg"
            android:hint="Write a note..." />

        <TextView
            android:id="@+id/setPriority"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textStyle="bold"
            android:text="Priority"
            android:layout_below="@id/new_edittext"/>


        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/setPriority">


            <me.tankery.lib.circularseekbar.CircularSeekBar
                android:id="@+id/seek_bar"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center_horizontal"
                android:layout_margin="10dp"
                app:cs_circle_stroke_width="48dp"
                app:cs_circle_style="butt"
                app:cs_negative_enabled="true"
                app:cs_pointer_angle="2"
                app:cs_pointer_stroke_width="48dp" />




                <TextView
                    android:id="@+id/seek_bar_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="30sp"
                    android:gravity="center"
                    android:layout_alignBottom="@id/seek_bar"
                    android:layout_alignTop="@id/seek_bar"
                    android:text="1" />


        </RelativeLayout>


